stopAt          endTime;

endTime         0.01;

deltaT          3e-4;

//writeControl    adjustableRunTime;
writeControl    timeStep;

//writeInterval   100;
writeInterval   10;

purgeWrite      0;

writeFormat     ascii;

writePrecision  10;

writeCompression off;

timeFormat      general;

timePrecision   8;

runTimeModifiable no;

